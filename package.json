    "babel-eslint": "^7.1.1",
    "babel-jest": "20.0.3",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "1.9.2",
    "babel-register": "^6.16.3",
    "bplist-creator": "0.0.7",
